{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "__typename": {
                "type": "string"
            },
            "description": {
                "type": "string"
            },
            "disciplines": {
                "type": "object",
                "properties": {
                    "__typename": {
                        "type": "string"
                    },
                    "color": {
                        "type": "string"
                    },
                    "numberOfMembers": {
                        "type": "integer"
                    },
                    "timeAllocations": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "__typename": {
                                    "type": "string"
                                },
                                "endDate": {
                                    "type": "string"
                                },
                                "partialTime": {
                                    "type": "boolean"
                                },
                                "startDate": {
                                    "type": "string"
                                },
                                "uuid": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "__typename",
                                "endDate",
                                "partialTime",
                                "startDate",
                                "uuid"
                            ]
                        }
                    },
                    "title": {
                        "type": "string"
                    },
                    "uuid": {
                        "type": "string"
                    }
                },
                "required": [
                    "__typename",
                    "color",
                    "numberOfMembers",
                    "timeAllocations",
                    "title",
                    "uuid"
                ]
            },
            "endDate": {
                "type": "string"
            },
            "numberOfDisciplines": {
                "type": "integer"
            },
            "projects": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "__typename": {
                            "type": "string"
                        },
                        "logo": {
                            "type": "string"
                        },
                        "title": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "__typename",
                        "logo",
                        "title"
                    ]
                }
            },
            "slug": {
                "type": "string"
            },
            "startDate": {
                "type": "string"
            },
            "title": {
                "type": "string"
            },
            "uuid": {
                "type": "string"
            }
        },
        "required": [
            "__typename",
            "description",
            "disciplines",
            "endDate",
            "numberOfDisciplines",
            "projects",
            "slug",
            "startDate",
            "title",
            "uuid"
        ]
    }
}